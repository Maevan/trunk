    <servlet-class>com.mapbar.sso.servlet.Token</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Token</servlet-name>
    <url-pattern>/token</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Loggot</display-name>
    <servlet-name>Loggot</servlet-name>
    <servlet-class>com.mapbar.sso.servlet.Loggot</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Loggot</servlet-name>
    <url-pattern>/loggot</url-pattern>
  </servlet-mapping>
</web-app>